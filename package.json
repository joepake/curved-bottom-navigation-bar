{
  "name": "curved-bottom-navigation-bar",
  "version": "2.0.2",
  "description": "Hight performance animated bottom navigation bar for both Android and IOS",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "src",
    "lib",
    "android",
    "!lib/typescript/example",
    "!android/build"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "react-navigation",
    "reanimated",
    "tab-bar",
    "bottombar",
    "bottom-bar",
    "reanimated-bottom-bar"
  ],
  "repository": "https://github.com/hngocl/curved-bottom-navigation-bar",
  "author": "HNgocL <lhngoc2497@gmail.com> (https://github.com/hngocl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hngocl/curved-bottom-navigation-bar/issues"
  },
  "homepage": "https://github.com/hngocl/curved-bottom-navigation-bar#readme",
  "dependencies": {
    "react-fast-compare": "3.1.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^14.1.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-navigation/bottom-tabs": ">=6.0.9",
    "@react-navigation/native": ">=6.0.6",
    "@release-it/conventional-changelog": "^3.3.0",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.34",
    "@types/react-native": "0.66.3",
    "commitlint": "^14.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "pod-install": "^0.1.28",
    "prettier": "^2.4.1",
    "react": "17.0.2",
    "react-native": "0.66.2",
    "react-native-builder-bob": "^0.18.2",
    "react-native-reanimated": "2.2.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-svg": "^12.1.1",
    "release-it": "^14.11.6",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "react": "*",
    "react-native": "*",
    "react-native-reanimated": "^2.2.4",
    "react-native-safe-area-context": ">=3.3.2",
    "react-native-svg": ">=12.1.1"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
